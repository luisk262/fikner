{% for entity in entities %}
    <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6">
        <h1>{{ entity.idAgencia.nombre_agencia|upper }}</h1>
        <a href="{{path('Agencia_page',{'nombre':entity.idAgencia.nombre_agencia|replace({' ':'_'})})}}">
        {% if entity.idPhoto.image == '' %}
            <img data-src="{{ asset('upload/default/default.jpg')}}"  src="{{ asset('upload/default/default.jpg')}}"> 
        {% else %}
            <img data-src="{{web_image('upload/Photos/' ~ entity.idPhoto.id ~'/' ~ entity.idPhoto.image).resize(450,450)}}"  src="{{web_image('upload/Photos/' ~ entity.idPhoto.id ~'/' ~ entity.idPhoto.image).resize(450,540)}}" > 
        {%endif%}
        </a>
    </div>
{% endfor %}






