        {% if entity.image == '' %}
            <img data-src="{{ web_image('upload/default/default.jpg').zoomCrop(350,262)}}"  src="{{ web_image('upload/default/default.jpg').zoomCrop(350,262)}}" class="img-responsive">
        {% else %}
            <img data-src="{{web_image('upload/Photos/' ~ entity.id ~'/' ~ entity.image).zoomCrop(350,262)}}"  src="{{web_image('upload/Photos/' ~ entity.id ~'/' ~ entity.image).zoomCrop(350,262)}}"  class="img-responsive">
        {%endif%}



