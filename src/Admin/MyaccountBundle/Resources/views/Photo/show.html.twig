{% extends "AdminMyaccountBundle::layout.html.twig" %}
{% block container %}
    <div class="col-lg-12 page-header">
        <div class="col-lg-10">
        </div>
        <div class="col-lg-2">
            {{ form(delete_form) }}
        </div>
    </div>
    <div class="col-lg-12">
        <img u=image height="auto" width="100%" src="{{web_image('upload/Photos/' ~ entity.id ~'/' ~entity.image)}}" />

    </div>
    <div class="col-lg-12">
        <label>Fecha creacion:</label> {{ entity.fecha|date('Y-m-d H:i:s') }}
        <label>Fecha de actualizacion:</label>                                             <th>Fechaupdate</th>
            {{ entity.fechaupdate|date('Y-m-d H:i:s') }}
    </div>
{%endblock%}

