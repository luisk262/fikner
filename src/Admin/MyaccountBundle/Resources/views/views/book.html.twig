<!DOCTYPE html>
<html lang="es">
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>Fikner - Book {{id}}</title>
        <meta name="description" content="">
        {% if photos %}
            {% if entities[0].image == '' %}
                <link rel="icon" type="image/jpeg" href="{{ asset('upload/default/default.jpg')}}" />
            {% else %}
                <link rel="icon" type="image/jpeg" href="{{web_image('upload/Photos/' ~ entities[0].id ~'/' ~ entities[0].image).resize(100,100)}}" />
            {%endif%}
            {% if HojadevidaP[0].idHojadevida.id != null %}
                {% set number1 = HojadevidaP[0].idHojadevida.fechaNac|date("Y") %}
                {% set number2 = "now"|date("Y") %}
                {% set aux= ((number2 - number1)) %}
                <meta property="og:description" content="Edad:{{aux}} Estatura:{{HojadevidaP[0].idHojadevida.estatura}}Cm Cabello:{{HojadevidaP[0].idHojadevida.pelo}}  Piel:{{HojadevidaP[0].idHojadevida.piel}} Ojos:{{HojadevidaP[0].idHojadevida.ojos}} Peso:{{HojadevidaP[0].idHojadevida.peso}}Kg Camisa:{{HojadevidaP[0].idHojadevida.tallaCamisa}} Pantalon:{{HojadevidaP[0].idHojadevida.tallaPantalon}} Calzado:{{HojadevidaP[0].idHojadevida.tallaZapato}}">
            {% else %}
                <meta property="og:description" content="No existe este book en fikner.com">
            {%endif%}

        {%else%}
            <link rel="icon" type="image/jpeg" href="{{ asset('upload/default/default.jpg')}}" />
        {%endif%}
        <link rel="stylesheet" href="{{asset('css/bootstrap/bootstrap.min.css')}}">
        <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
        <link href="{{asset('css/page-book/ie10-viewport-bug-workaround.css')}}" rel="stylesheet">
        <script src="{{asset('js/page-book/ie-emulation-modes-warning.js')}}"></script>
        <link href="{{asset('css/page-book/carousel.css')}}" rel="stylesheet">
        <link href="{{asset('css/page-book/style.css')}}" rel="stylesheet">

    </head>
    <body>
        <nav class="navbar navbar-default">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="{{path('principal')}}"><img src="{{asset('images/logo/fikner_logo_blanco.png')}}" alt="" height="100%"></a>
                </div>
            </div>
        </nav>
        <div class="container marketing">
            <div class="row featurette">
                <div class="col-md-7">
                    <h2 class="featurette-heading">Ciudad de residencia: <span class="text-muted">{{HojadevidaP[0].idHojadevida.ciudadresidencia}}</span></h2>
                    <h2 class="featurette-heading">Edad: <span class="text-muted">{{aux}} <small>a√±os</small></span></h2>
                </div>
                <div class="col-md-5">
                   {%set contador=0%}
                     {%set aux=false%}
                     {% for entity in entities %}
                        {%if contador==0%}
                            {% if entity.image == ''%}
                                {%set aux=true%}
                                <img class="featurette-image img-responsive center-block" data-src="{{ asset('upload/default/default.jpg')}}" alt="Image " src="{{ asset('upload/default/default.jpg')}}" data-holder-rendered="true">
                            {%else%}
                                 {%set aux=true%}
                                <img class="featurette-image img-responsive center-block" data-src="{{web_image('upload/Photos/' ~ entity.id ~'/' ~ entity.image).resize(500,500)}}" alt="Image " src="{{web_image('upload/Photos/' ~ entity.id ~'/' ~ entity.image).resize(500,500)}}" data-holder-rendered="true">
                            {%endif%}
                        {%endif%}
                        {% set contador=contador+1%}
                    {%endfor%}
                    {%if not aux %}
                            <img class="featurette-image img-responsive center-block" data-src="{{ asset('upload/default/default.jpg')}}" alt="Image " src="{{ asset('upload/default/default.jpg')}}" data-holder-rendered="true">
                    {%endif%}
