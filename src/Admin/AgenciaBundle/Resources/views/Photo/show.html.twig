{% extends "AdminAgenciaBundle::layout.html.twig" %}
{% block container %}
    <div class="col-lg-12">
        <p>
            {% set anioF = entity.fecha|date('Y') %}
            {% set anioN = "now"|date("Y") %}
            {% set aux= ((anioN - anioF)) %}
            {% if aux >0 %}
                {% set mesF = entity.fecha|date('m') %}
                {% set mesN = "now"|date("m") %}
                {% set meses= ((12-mesF)) %}
                {% set aux= ((mesN + meses)) %}
                <label for="antiguedad">Antiguiedad de Foto:</label> {{ aux }} meses.
            {% else %}
                {% set mesF = entity.fecha|date('m') %}
                {% set mesN = "now"|date("m") %}
                {% set aux= ((mesN - mesF)) %}
                <label for="antiguedad">Antiguiedad de Foto:</label> {{ aux }} meses.
            {% endif %}
        </p>        <label>Fecha de subida a fikner:</label> {{ entity.fecha|date('Y-m-d') }}
    </div>
    <div class="col-lg-12">

        <img u=image height="auto" width="100%" src="{{web_image('upload/Photos/' ~ entity.id ~'/' ~ entity.image)}}" />
    </div>

{%endblock%}

