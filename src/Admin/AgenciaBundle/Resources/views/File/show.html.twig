{% extends "AdminMyaccountBundle::layout.html.twig" %}
{% block body %}
    {% block menu %}
        {{parent()}}
    {%endblock%}
    <div class="contenido">
        <div class="container">            
            <div class="col-lg-12 cuadro-azul">
                <div class="formulario">
                    <div class="col-lg-12">
                        <center>
                            {% block container %}
                                <div class="col-lg-12 page-header">
                                        <div class="col-lg-10">
                                        </div>
                                        <div class="col-lg-2">
                                            {{ form(delete_form) }}
                                        </div>
                                </div>


                                <div class="col-lg-12">
                                    <img u=image height="auto" width="100%" src="{{asset('upload/Photos/' ~ entity.id ~'/' ~entity.image)}}" />

                                </div>
                                <div class="col-lg-12">
                                    <label>Fecha creacion:</label> {{ entity.fecha|date('Y-m-d H:i:s') }}
                                    <label>Fecha de actualizacion:</label>                                             <th>Fechaupdate</th>
                                        {{ entity.fechaupdate|date('Y-m-d H:i:s') }}

                                </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    {%endblock%}
    {% block javascripts %}
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous"></script>
    {% endblock javascripts %}

{%endblock%}

