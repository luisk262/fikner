{% for entity in entities %}
    {% if entity.image == '' %}
        <div class="col-lg-4 col-md-4 col-xs-6 thumb">
                <a class="thumbnail" href="{{ path('Myaccount_photo_show', { 'id': entity.id }) }}">
                <img class="img-responsive"  width="400px" height="90%"src="{{ asset('upload/default/default.jpg')}}" alt="">
                <span>Ups! Intenta de nuevo</span>
            </a>
        </div>
            {% else %}
        <div class="col-lg-4 col-md-4 col-xs-6 thumb">
            <a class="thumbnail" href="{{ path('Myaccount_photo_show', { 'id': entity.id }) }}">
                <img class="img-responsive" src="{{web_image('upload/Photos/' ~ entity.id ~'/' ~ entity.image).resize(400,400)}}" alt="">
            </a>
        </div>
    {%endif%}
{% endfor %}



